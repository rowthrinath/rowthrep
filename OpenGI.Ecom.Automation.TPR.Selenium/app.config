<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="seleniumSettings" type="OpenGI.Selenium.Configuration.SeleniumSettingsSection, OpenGI.Selenium" />
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <appSettings>
    <add key="BaseUrl" value="https://test-towergatetpr.wnet.local/CoreQuote/ProposerDetails.dq?Product=26&amp;Style=0" />
    <add key="timeToSleep" value="20" />
    <add key="AccordionTransitionWait" value="5000" />
  </appSettings>
  <seleniumSettings>
    <!-- Browser: PhantomJs, GoogleChrome -->
    <browser selected="GoogleChrome" />
  </seleniumSettings>
  <specFlow>
    <stepAssemblies>
      <stepAssembly assembly="OpenGI.Ecom.Automation.Selenium.Framework" />
    </stepAssemblies>
    <!-- Possible values include NUnit (default), MsTest, xUnit -->
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
    <unitTestProvider name="NUnit" />
    <!-- Other SpecFlow Configuration options. For additional details on the options specified here, 
      see http://cloud.github.com/downloads/techtalk/SpecFlow/SpecFlow%20Guide.pdf
    <language feature="en-US" tool="" />
    
    <generator allowDebugGeneratedFiles="false" />
    
    <runtime detectAmbiguousMatches="true"
      stopAtFirstError="false"
      missingOrPendingStepsOutcome="Inconclusive" />
    
    <trace traceSuccessfulSteps="true"
      traceTimings="false"
      minTracedDuration="0:0:0.1"
      listener="TechTalk.SpecFlow.Tracing.DefaultListener,TechTalk.SpecFlow" />
    -->
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
  </specFlow>
</configuration>